 <div class="container">
        <header id="topoFixo">
            <div id="logoMenu">
                <div id="logo">J-rss Reader</div>
            </div>
            <div id="searchBar">
                <form action="" method="GET" autocomplete="off">
                    <input type="text" name="" id="" placeholder="Procure seus feeds por tags">
                    <input type="submit" value="">
                </form>
            </div>
            <div id="privateInfo">
                <ul class="dropmenu">    
                    <li>
                        <a href="#" id="nome">Olá, {{nome}}</a>
                        <ul>
                            <li> <a href="/opçoes">Configurações</a> </li>
                            <li>   <a href="/logout">Sair</a> </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </header>
        <div id="content">
            <div class="rssOp">
                   <button id="createFeed">Adicionar um novo feed</button>
            </div>
            {{#each rssList}}
               <div class="rssOp contentRequest" data-id="{{this._id}}">
                   <h1 class="truncate"> Titulo: {{this.nome}}</h1>
                   <h2 class="linkRss truncate"> Link do feed: {{this.url}}</h2>
                   <span class="tags truncate"> <strong>Tags: </strong>{{this.tags}}</span>
                   <button class="deleteFeed"> Excluir Feed</button>
               </div>
        {{/each}}
        
        </div>
        <div id="paginaCadastro">
                <form id="addRss" autocomplete="on">

                    <label for="nome">Nome:</label>
                    <input type="text" name="nome" id="nomeFeed" placeholder="Digite o nome do feed" required>

                    <label for="url">URL:</label>
                    <input type="text" name="url" id="urlFeed" autofocus placeholder="Digite a url do feed" required>

                    <label for="tags">Tags:</label>
                    <input type="text" name="tags" id="tagsFeed" placeholder="Digite as tags separadas por  ',' " required>

                    <input id="submitFeedButton" type="submit" value="Cadastrar feed">
                    <input type="button" value="Fechar" id="closeFeed">
                </form>
        </div>
    </div>
    <script src="../JS/scriptHome.js"></script>
    </script>